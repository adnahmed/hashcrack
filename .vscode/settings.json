{
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.formatOnSave": true,
  "stylelint.configFile": ".stylelintrc.json",
  "prettier.enable": true,
  "prettier.singleQuote": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "launch": {
    "configurations": [],
    "compounds": []
  }
}